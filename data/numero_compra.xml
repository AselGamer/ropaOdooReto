<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="sequencia_numero_compra" model="ir.sequence">
            <field name="name">Sequencia del numero de compra</field>
            <field name="implementation">no_gap</field>
            <field name="code">sequencia.compra</field>
            <field name="prefix">NC-</field>
            <field name="suffix">-%(year)s</field>
